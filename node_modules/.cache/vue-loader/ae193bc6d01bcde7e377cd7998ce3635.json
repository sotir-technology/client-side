    this.$refs.topbar.start();\n    this.dataLoading = true;\n    this.btnText = \"Processing...\";\n    //start network call\n    this.axios.post(CFG.API_URI + '/auth/login/', {\n        ssk: CFG.SSK,\n        email: this.model.email,\n        password: this.model.password\n    })\n        .then((res) => {\n            //check success\n            if (res.data.status) {\n                //is logged\n                if (res.data.data.s_token != null) {\n                    //store login details\n                    this.$cookie.set(CFG.COOKIE_NAME, res.data.data.s_token, 5);\n                    this.$router.push({name: 'dashboard'});\n                }\n                return;\n            }\n            this.msg = res.data.msg;\n            this.$refs.topbar.done();\n            this.dataLoading = false;\n            this.btnText = \"Re-Login\";\n        });\n}\n",{"version":3,"sources":["Login.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsDA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Login.vue","sourceRoot":"src/views","sourcesContent":["<template>\n    <div class=\"row justify-content-center\">\n        <vue-topprogress ref=\"topbar\" color=\"#FF0078\"></vue-topprogress>\n        <div class=\"col-lg-5 col-md-7\">\n            <div class=\"card bg-secondary shadow border-0\">\n                <div class=\"card-header bg-transparent pb-5\">\n                    <div class=\"btn-wrapper text-center\">\n                        <img width=\"120\" src=\"img/brand/logow.png\"/>\n                    </div>\n                </div>\n                <div class=\"card-body px-lg-5 py-lg-5\">\n                    <div class=\"text-center text-muted mb-4\">\n                        <small>Sign in with credentials</small>\n                    </div>\n                    <form role=\"form\">\n                        <base-input :disabled=\"dataLoading\" class=\"input-group-alternative mb-3\"\n                                    placeholder=\"Email\"\n                                    addon-left-icon=\"ni ni-email-83\"\n                                    v-model=\"model.email\">\n                        </base-input>\n\n                        <base-input :disabled=\"dataLoading\" class=\"input-group-alternative\"\n                                    placeholder=\"Password\"\n                                    type=\"password\"\n                                    addon-left-icon=\"ni ni-lock-circle-open\"\n                                    v-model=\"model.password\">\n                        </base-input>\n                        <small>{{msg}}</small>\n                        <base-checkbox class=\"custom-control-alternative\">\n                            <span class=\"text-muted\">Remember me</span>\n                        </base-checkbox>\n                        <div class=\"text-center\">\n                            <base-button ref=\"btn\" :disabled=\"dataLoading\" v-on:click=\"btnLogin\" type=\"danger\"\n                                         class=\"my-5\">{{btnText}}\n                            </base-button>\n                        </div>\n                    </form>\n                </div>\n            </div>\n            <div class=\"row mt-3\">\n                <div class=\"col-6\">\n                    <router-link to=\"forgot-pw\">\n                        <small class=\"text-white\">Forgot Password</small>\n                    </router-link>\n                </div>\n                <div class=\"col-6 text-right\">\n                    <small style=\"cursor: default\">RSC Byte Limited</small>\n                </div>\n            </div>\n        </div>\n        <HeaderMeta title=\"Login\"/>\n    </div>\n</template>\n<script>\n    import {CFG} from './../ServerKeys';\n    import HeaderMeta from \"./HeaderMeta\";\n\n    export default {\n        mounted() {\n            this.$refs.topbar.start();\n            setTimeout(() => {\n                this.$refs.topbar.done();\n            }, 300);\n        },\n        name: 'login',\n        components: {HeaderMeta},\n        data() {\n            return {\n                msg: '',\n                btnText: 'Login Now',\n                dataLoading: false,\n                model: {\n                    email: '',\n                    password: ''\n                }\n            }\n        },\n        methods: {btnLogin: btnLogin}\n    }\n\n    //start login thing\n    function btnLogin() {\n        this.msg = '';\n        this.$refs.topbar.start();\n        this.dataLoading = true;\n        this.btnText = \"Processing...\";\n        //start network call\n        this.axios.post(CFG.API_URI + '/auth/login/', {\n            ssk: CFG.SSK,\n            email: this.model.email,\n            password: this.model.password\n        })\n            .then((res) => {\n                //check success\n                if (res.data.status) {\n                    //is logged\n                    if (res.data.data.s_token != null) {\n                        //store login details\n                        this.$cookie.set(CFG.COOKIE_NAME, res.data.data.s_token, 5);\n                        this.$router.push({name: 'dashboard'});\n                    }\n                    return;\n                }\n                this.msg = res.data.msg;\n                this.$refs.topbar.done();\n                this.dataLoading = false;\n                this.btnText = \"Re-Login\";\n            });\n    }\n</script>\n<style>\n</style>\n"]}]}